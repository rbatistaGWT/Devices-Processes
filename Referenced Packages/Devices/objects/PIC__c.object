        <masterLabel>Email PI Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/apex.js&quot;)} 


var piID=&apos;{!PIC__c.Id}&apos;;
var st = sforce.apex.execute(&quot;greatwave2.emailNotifyPI&quot;,&quot;sendNotifyPI&quot;,{piID:piID});
alert(st);
window.location.reload(true);</url>

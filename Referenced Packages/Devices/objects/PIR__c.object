    <validationRules>
        <fullName>Due_Date_Earlier_than_Start_Date</fullName>
        <active>false</active>
        <description>This rule enforces the Due Date must be greater than Start Date.</description>
        <errorConditionFormula>Due_Date__c &lt;=  Start_Date__c</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>The Due Date must be greater than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Notification</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c &lt;  First_Notification__c</errorConditionFormula>
        <errorDisplayField>First_Notification__c</errorDisplayField>
        <errorMessage>First Notification Date should be prior the Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fourth_Notification</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNULL(Third_Notification__c)) &amp;&amp; Third_Notification__c &lt; Fourth_Notification__c</errorConditionFormula>
        <errorDisplayField>Fourth_Notification__c</errorDisplayField>
        <errorMessage>Fourh Notification date must be after Third Notification date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_Date_Validation</fullName>
        <active>true</active>
        <description>This validation enforces the Request Date cannot be greater than the Start Date or Due Date</description>
        <errorConditionFormula>Request_Date__c &gt; Start_Date__c || Request_Date__c &gt; Due_Date__c</errorConditionFormula>
        <errorDisplayField>Request_Date__c</errorDisplayField>
        <errorMessage>The Request Date cannot be greater than the Start or Due Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Second_Notification</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNULL(First_Notification__c)) &amp;&amp; First_Notification__c &lt; Second_Notification__c</errorConditionFormula>
        <errorDisplayField>Second_Notification__c</errorDisplayField>
        <errorMessage>Second Notification Date must be after the First Notification Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Third_Notification</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNULL(Second_Notification__c)) &amp;&amp; Second_Notification__c &lt; Third_Notification__c</errorConditionFormula>
        <errorMessage>Third Notification Date must be after Second Notification Date</errorMessage>
    </validationRules>
</CustomObject>

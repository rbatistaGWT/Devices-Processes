    <validationRules>
        <fullName>DOB_Must_be_in_the_past</fullName>
        <active>false</active>
        <description>Prevents patient with a DOB on the future.</description>
        <errorConditionFormula>Patient_DOB__c &gt;= TODAY() &amp;&amp; NOT(ISBLANK(Patient_DOB__c))</errorConditionFormula>
        <errorDisplayField>Patient_DOB__c</errorDisplayField>
        <errorMessage>Patient DOB must be in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Patient_Number_cannot_change</fullName>
        <active>false</active>
        <description>Does not allow the user to change the Patient Number. This number is automatically populated via a Workflow rule</description>
        <errorConditionFormula>ISCHANGED(Name)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You Cannot change the Patient Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Format</fullName>
        <active>false</active>
        <description>Ensures the format of the phone is (xxx) xxx-xxxx</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Patient_Phone_Number__c)),NOT(REGEX( Patient_Phone_Number__c , &quot;\\([0-9]{3}\\) [0-9]{3}\\-[0-9]{4}&quot;)))</errorConditionFormula>
        <errorMessage>the Phone field should be in the following format : (xxx) xxx-xxxx</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_AddressLine1_on_Status_Verified</fullName>
        <active>false</active>
        <description>Enforces fields being required if the Status = &quot;Verified&quot;</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,&quot;Verified&quot;),ISBLANK(Address_Line_1__c))</errorConditionFormula>
        <errorMessage>You must input the Patient’s Address Line 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_City_On_Status_Verified</fullName>
        <active>false</active>
        <description>Enforces required fields when Status = &quot;Verified&quot;</description>
        <errorConditionFormula>AND(
ISPICKVAL(Status__c,&quot;Verified&quot;),
ISBLANK(City__c)
)</errorConditionFormula>
        <errorMessage>You must input the Patient’s City</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Country_On_Status_Verified</fullName>
        <active>false</active>
        <description>AND(ISPICKVAL(Status__c,&quot;Verified&quot;),OR(
 ISBLANK(Address_Line_1__c),
 ISBLANK(City__c),
 ISBLANK(State__c),</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,&quot;Verified&quot;),
 ISBLANK(Country__c))</errorConditionFormula>
        <errorMessage>You must input the Patient&apos;s Country</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_PatientGender_Status_Verified</fullName>
        <active>false</active>
        <description>Enforces required fields when Status = &quot;Verified&quot;</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,&quot;Verified&quot;),
 ISBLANK(TEXT(Patient_Gender__c)))</errorConditionFormula>
        <errorMessage>You must input the Patient&apos;s Gender.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_PatientNumber_Status_Verified</fullName>
        <active>false</active>
        <description>Enforces required fields when Status = &quot;Verified&quot;</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,&quot;Verified&quot;),
 Patient_Phone_Number__c == &apos;&apos;)</errorConditionFormula>
        <errorMessage>You must input Patient&apos;s Phone Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_PostalCode_Status_Verified</fullName>
        <active>false</active>
        <description>Enforces required fields when Status = &quot;Verified&quot;</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,&quot;Verified&quot;),
 ISBLANK(Postal_Code__c))</errorConditionFormula>
        <errorMessage>You must input the Patient&apos;s Postal Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_State_On_Status_Verified</fullName>
        <active>false</active>
        <description>Enforces required fields when Status = &quot;Verified&quot;</description>

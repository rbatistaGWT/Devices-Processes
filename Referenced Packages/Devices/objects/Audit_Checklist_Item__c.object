        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)}

var checklistItemToUpdate = new sforce.SObject(&quot;greatwave2__Audit_Checklist_Item__c&quot;);
checklistItemToUpdate.Id = &apos;{!Audit_Checklist_Item__c.Id}&apos;;
//checklistItemToUpdate.OwnerId = &apos;{!$User.Id}&apos;;
checklistItemToUpdate.greatwave2__Start_Time__c  = new Date().toISOString();
var result = sforce.connection.update([checklistItemToUpdate]);

if (result[0].success==&apos;false&apos;) {
alert(result[0].errors.message);
} else {
location.reload(true);
}</url>

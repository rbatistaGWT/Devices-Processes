    <validationRules>
        <fullName>Order_Status_New</fullName>
        <active>false</active>
        <description>Sales Rep is not allowed to change Order Status from another stage back to &quot;new&quot;.</description>
        <errorConditionFormula>AND(OR( 
ISCHANGED(Status__c), ISNEW() ),OR( 
ISPICKVAL(Status__c, &apos;Shipped&apos; ) ,
ISPICKVAL(Status__c, &apos;Received&apos; ) , 
ISPICKVAL(Status__c , &apos;Submitted&apos;) , 
ISPICKVAL(Status__c , &apos;In Process&apos;)), 
$Profile.Name != &apos;System Administrator&apos;)</errorConditionFormula>
        <errorMessage>Sales user is not able to change status back to new from another stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Type_Cannot_Be_None</fullName>
        <active>false</active>
        <description>Prevent Order Type from being &quot;None&quot;</description>
        <errorConditionFormula>TEXT(Order_Type__c) == &apos;&apos;</errorConditionFormula>
        <errorDisplayField>Order_Type__c</errorDisplayField>
        <errorMessage>Please Select a valid Order Type.</errorMessage>
    </validationRules>
